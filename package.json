{
  "name": "realtime-ww2-alexa",
  "version": "0.1.0",
  "description": "Realtime WW2",
  "scripts": {
    "pretest": "standard",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:integration": "AWS_REGION=eu-west-1 mocha --compilers js:babel-core/register --require babel-polyfill packages/aws-integration-test/integration/**/*.spec.js",
    "test:acceptance": "AWS_REGION=eu-west-1 mocha --compilers js:babel-core/register --require babel-polyfill packages/aws-integration-test/acceptance/**/*.spec.js",
    "test:all": "lerna run test && npm run test:integration && npm run test:acceptance"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ceilfors/realtime-ww2-alexa.git"
  },
  "devDependencies": {
    "alexa-sdk": "^1.0.14",
    "aws-sdk": "^2.115.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "copy-webpack-plugin": "^4.0.1",
    "lerna": "^2.2.0",
    "mocha": "^3.5.3",
    "nock": "^9.0.14",
    "serverless": "^1.22.0",
    "serverless-crypt": "0.0.3",
    "serverless-webpack": "^2.2.0",
    "sinon": "^3.2.1",
    "standard": "^10.0.3",
    "webpack": "^3.3.0"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "author": "ceilfors",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ceilfors/realtime-ww2-alexa/issues"
  },
  "homepage": "https://github.com/ceilfors/realtime-ww2-alexa#readme",
  "private": true
}
